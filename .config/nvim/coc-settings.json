{
  "diagnostic.warningSign": " ",
  "diagnostic.errorSign": " ",
  "list.indicator": " ",
  "list.selectedSignText": " ",
  "languageserver":
  {
      "pyls": {
          "command": "pyls",
          "filetypes": ["py"]
      },
      "rustup": {
          "command": "/usr/bin/rustup",
          "args": ["run", "stable", "rls"],
          "filetypes": ["rust"]
      },
      "bash": {
          "command": "bash-language-server",
          "args": ["start"],
          "filetypes": ["sh"],
          "ignoredRootPaths": ["~"]
      }
  }
}
