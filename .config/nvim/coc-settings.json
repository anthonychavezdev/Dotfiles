{
  "diagnostic.warningSign": " ",
  "diagnostic.errorSign": " ",
  "list.indicator": " ",
  "list.selectedSignText": " ",
  "languageserver":
  {
      "pyls": {
          "command": "pyls",
          "filetypes": ["py"]
      },
      "cquery": {
          "command": "/usr/bin/cquery",
          "args": ["--log-file=/tmp/cquery.log"],
          "initializationOptions": {
                  "cacheDirectory": "/tmp/cquery"
                  },
          "filetypes": ["c", "cpp", "cc"]
      },
      "rustup": {
          "command": "/usr/bin/rustup",
          "args": ["run", "stable", "rls"],
          "filetypes": ["rust"]
      },
      "javascript-typescript-stdio": {
          "command": "/usr/local/bin/javascript-typescript-stdio",
          "filetypes": ["js", "ts"]
      }
  }
}
