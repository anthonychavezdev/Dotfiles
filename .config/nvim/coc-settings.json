{
  "diagnostic.warningSign": " ",
  "diagnostic.errorSign": " ",
  "list.indicator": " ",
  "list.selectedSignText": " ",
  "languageserver":
  {
      "python": {
          "command": "pyls",
          "filetypes": ["py"]
      },
      "c": {
          "command": "/usr/bin/cquery",
